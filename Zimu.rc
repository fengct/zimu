        MENUITEM "复制(&C)\tCtrl+C",              ID_EDIT_COPY
        MENUITEM "粘贴(&P)\tCtrl+V",              ID_EDIT_PASTE
        MENUITEM SEPARATOR
        MENUITEM "错误检查&E)\tCtrl+E",             IDM_CHECK
        MENUITEM SEPARATOR
        MENUITEM "查找(&F)\tCtrl+F",              IDM_FIND
        MENUITEM "替换(&R)\tCtrl+R",              IDM_REPLACE
        MENUITEM SEPARATOR
        MENUITEM "时间对齐(&A)",                    ID_EDIT_TIMEGAP
        MENUITEM "时间偏移(&O)",                    ID_TIME_OFFSET
    END
    POPUP "查看(&V)"
    BEGIN
        MENUITEM "工具栏(&T)",                     ID_VIEW_TOOLBAR
        MENUITEM "状态栏(&S)",                     ID_VIEW_STATUS_BAR
    END
    POPUP "帮助(&H)"
    BEGIN
        MENUITEM "关于 Zimu(&A)...",              ID_APP_ABOUT
    END
    MENUITEM "调试",                          IDM_DEBUG
END


